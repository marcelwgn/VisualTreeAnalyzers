<Project>
  <PropertyGroup>
    <!--
      CA1303: Do not pass literals as localized parameters
      Ignore reason: All strings we use are templates or logging and are in english per design

      CA1305: Specify IFormatProvider
      Ignore reason: All strings are english
      
      WMC1501: Property is for evaluation purposes
      Ignore reason: We are fine with that here
      
      CS8305: Property is for evaluation purposes
      Ignore reason: We are fine with that here

      CA1716: Identifiers should not match keywords.
      Ignore reason: Where identifiers match keywords, they are extremely unlikely to be mistaken for the keyword. 
      -->

    <NoWarn>;$(NoWarn);CA1303;CA1305;WMC1501;CS8305;CA1716</NoWarn>
  </PropertyGroup>
</Project>
